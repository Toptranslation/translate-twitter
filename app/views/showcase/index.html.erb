<div class="span-24 last">Header</div>
<div class="span-4">
  <ul>
    <% @users.each do |user| %>
    <li>
      <%= user.username %>
    </li>
    <% end %>
  </ul>
</div>
<div class="span-16">
  <ul>
    <% @tweets.each do |tweet| %>
    <li>
      <%= tweet.text %>
    </li>
    <% end %>
  </ul>
</div>
<div class="span-4 last">Right Sidebar</div>
<div class="span-24 last">Footer</div>
